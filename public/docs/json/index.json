          "type": "integer",
          "format": "int64"
        },
        "user_name": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "real name": {
          "type": "string"
        },
        "mobile": {
          "type": "string"
        },
        "status": {
          "type": "integer",
          "description": "admin status",
          "enum": [
            "0",
            "1"
          ]
        }
      },
      "xml": {
        "name": "admin"
      }
    },
    "password": {
      "type": "object",
      "required": [
        "originPassword",
        "newPassword"
